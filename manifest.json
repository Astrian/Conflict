{
    "name": "Conflict",
    "description": "A RSS Reader.",
    "version": "0.1",
    "permissions": [
        "storage"
    ],
    "sockets": {
        "tcp": {
            "connect": [":*"]
        }
    },
    "icons":{
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "app":{
        "background": {
            "scripts": ["conflict.js"],
            "persistent": false
        }
    },
    "manifest_version": 2
}